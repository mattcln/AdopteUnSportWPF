<Window x:Class="AdopteUnSportWPF.Client"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AdopteUnSportWPF"
        mc:Ignorable="d"
        Title="Nouveau Client" Height="450" Width="800">
    <StackPanel>

        <Label Content="Nom"/>
        <TextBox Height="23" Width="220" TextWrapping="Wrap" Text="" Margin="0,0,550,0"/>
        <Label Content="Prénom"/>
        <TextBox Height="23" Width="220" TextWrapping="Wrap" Text="" Margin="0,0,550,0"/>
        <Label Content="Année de naissance"/>
        <TextBox Height="23" Width="220" TextWrapping="Wrap" Text="" Margin="0,0,550,0"/>
        <Label Content="Adresse"/>
        <TextBox Height="23" Width="220" TextWrapping="Wrap" Text="" Margin="0,0,550,0"/>
        <Label Content="Ville"/>
        <TextBox Height="23" Width="220" TextWrapping="Wrap" Text="" Margin="0,0,550,0"/>
        <Label Content="Adresse Email"/>
        <TextBox Height="23" Width="220" TextWrapping="Wrap" Text="" Margin="0,0,550,0"/>
        <Button x:Name="btnValiderC" Content="Valider" Height="23" Width="70" Margin="0,50,550,0" Padding="2"/>

    </StackPanel>
</Window>
